mkdir AlphaPoint25Beta1
cp *.* AlphaPoint25Beta1

cd AlphaPoint25Beta1
mkdir Set1
mkdir Set1/Txt
mkdir Set1/Images
sed -i 's/ALPHA,0.25/ALPHA,0.25/g' Cell_Heterogeneity.conf
sed -i 's/BETA,1/BETA,1/g' Cell_Heterogeneity.conf
cp *.* Set1

for num in {2..10}; do
    cp -r Set1 Set$num
done
cd ..


